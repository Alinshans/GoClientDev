<AnchorPane fx:id="loginPane" prefHeight="307.0" prefWidth="500.0" GridPane.columnIndex="1" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.main.view.LoginController">
    <children>
        <TextField fx:id="account" layoutX="118.0" layoutY="94.0" prefHeight="40.0" prefWidth="240.0" promptText="请输入您的帐号">
         <font>
            <Font size="20.0" />
         </font></TextField>
      <PasswordField fx:id="password" layoutX="118.0" layoutY="155.0" prefHeight="40.0" prefWidth="240.0" promptText="请输入您的密码">
         <font>
            <Font size="20.0" />
         </font></PasswordField>
      <Label fx:id="invaildMessageTips" layoutX="229.0" layoutY="209.0" text="无效信息提示" textFill="RED" />
        <Button fx:id="loginBtn" layoutX="99.0" layoutY="234.0" mnemonicParsing="false" onAction="#login" prefHeight="40.0" prefWidth="95.0" text="登陆">
            <font>
                <Font size="20.0" />
            </font></Button>
        <Button fx:id="registerBtn" layoutX="311.0" layoutY="234.0" mnemonicParsing="false" onAction="#signup" prefHeight="40.0" prefWidth="95.0" text="注册">
            <font>
                <Font size="20.0" />
            </font></Button>
      <Label fx:id="emptyAccountTips" layoutX="383.0" layoutY="114.0" text="请输入账号" textFill="RED" />
      <Label fx:id="emptyPasswordTips" layoutX="383.0" layoutY="175.0" text="请输入密码" textFill="RED" />
    </children>
</AnchorPane>
